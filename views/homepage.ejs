<html>
<title>
    Login
    </title>
<head>
    <link rel='stylesheet' href='stylesheets/Vidage.css' />
    <link rel='stylesheet' href='stylesheets/loginstyle.css' />
    <script src="javascript/jquery-2.2.4.min.js"></script>
    <script src="javascript/Vidage.min.js"></script>
    <script src="javascript/login-script.js"></script>
    </head>
<body>
    <div class="Vidage">
    <div class="Vidage__image"></div>

    <video id="VidageVideo" class="Vidage__video" preload="metadata" loop autoplay muted>
        <source src="videos/video.mp4" type="video/mp4">
    </video>

    <div class="Vidage__backdrop"></div>
</div>
    <!-- Vidage init -->

        <script>
            new Vidage('#VidageVideo');
        </script>
    
<div class="loginContainer fadeIn animated">
<div class="LogintopBar">
<div class="tb-search"><input type="search" placeholder="Search on SocialNet" ></div>
 <div class="tb-menus">
          <ul class="u-list">
          <li class="tb-link"><a href="#"><img class="icon icons8-Home" src="data:image/png;base64," width="24" height="24">Home</a></li>
              <li class="tb-link"><a href="#"><img class="icon icons8-Binoculars" src="data:image/png;base64," width="24" height="24">Discover</a></li>
              
              <li class="tb-link"><a href="#"><img class="icon icons8-Info" src="data:image/png;base64," width="24" height="24">About</a></li>
          </ul></div>
</div>



<!--Login page left div begin-->
     <div class="login-left-div">
    <h1 class="loginH1">Welcome to SocialNet.</h1>
<p class="loginparagraph">
Connect with your friends  and other fascinating people. Get in-the-moment updates on the things that interest you. And watch events unfold, in real time, from every angle.
</p>


    </div>
    <!--Login page left div end-->
    <!--Login page right div begin-->
  <div class="login-right-div">
  <div class="login-logodiv">
  <img class="icon icons8-Share-Filled" src="data:image/png;base64," width="50" height="50">
  <span>SocialNet</span>
  </div>
    <div class="LoginFormcont" id="login-form-cont">
    <span id="errorMessage"></span>
          <form id="loginForm"  action="/login" method="POST">
            <div class="username-error"><img class="icon icons8-Cancel" src="data:image/png;base64," width="30" height="30">
        </div>
                <div class="password-error"><img class="icon icons8-Cancel" src="data:image/png;base64," width="30" height="30">
</div>
        <input type="text" name="username" id="username" placeholder="Username or Email">
          <input type="password" name="password" id="password" placeholder="Password"> 
          <input type="submit" id="loginBtn" value="Log in" >
          <div class="loginOptions">
          <label><input type="checkbox" id="rememberme">Remember me.</label>
          <label><a href="#">Forgot password?</a></label>
          </div>
          
        
        </form>
      
      </div>
      <!--Login page loginformcont div end-->
    <div class="register-buttons">
<label>New to Socialnet ?</label>
<button class="registerbuttonclass button1" id="create-profile">
<img class="icon icons8-Add-User-Male" src="data:image/png;base64," width="40" height="40">
 Create your profile</button>
<label> or</label>
<button class="registerbuttonclass button2" id="create-page">
<img class="icon icons8-Template-Filled" src="data:image/png;base64," width="40" height="40">
Create your Page</button>
    </div>
    <!--Login page register buttons div end-->
    </div>
    <!--Login page right div end-->
   
    </div>
    <div class="login-footer"></div>
    </body>
</html>
